{
  "parser": "babel-eslint",
  "parserOptions": {
    "sourceType": "module"
  },
  "extends": ["eslint:recommended", "google"],
  "rules": {
    "no-console": 0,
    "require-jsdoc": 0,
    "valid-jsdoc": 0,
    "async-await/space-after-async": 2,
    "async-await/space-after-await": 2
  },
  "env": {
    "node": true
  },
  "plugins": ["async-await"]
}
