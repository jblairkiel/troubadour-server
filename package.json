{
  "name": "troubadour-server",
  "version": "1.0.0",
  "description": "",
  "main": "src/main.js",
  "scripts": {
    "eslint": "eslint src",
    "eslint-fix": "eslint --fix src && eslint --fix tests",
    "clean": "./node_modules/rimraf/bin.js ./out",
    "build-docs": "apidoc -i src/ -o out/docs/ -f \"controllers/.*\\.js\"",
    "pre-build": "npm run eslint-fix && npm run eslint && npm run clean",
    "build": "npm run pre-build && npm run build-docs && babel src --out-dir out/",
    "start": "NODE_ENV='production' node out/main.js",
    "start-dev": "NODE_ENV='development' babel-watch src/main.js",
    "test": "npm run start-dev && mocha tests/tests.js --reporter spec"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "apidoc": "^0.19.1",
    "babel-polyfill": "^6.26.0",
    "body-parser": "^1.17.0",
    "braces": "^3.0.2",
    "cache-manager": "^2.11.1",
    "cache-manager-redis": "^0.4.0",
    "chai": "^4.3.6",
    "core-js": "^3.23.3",
    "dotenv": "^4.0.0",
    "errorhandler": "^1.5.1",
    "es6-error": "^4.1.1",
    "express": "^4.18.1",
    "express-async-errors": "^2.1.2",
    "glob-parent": "^6.0.2",
    "levenshtein": "^1.0.5",
    "morgan": "^1.10.0",
    "node-datetime": "^1.0.1",
    "pg": "^6.4.2",
    "pg-hstore": "^2.3.4",
    "react": "^18.2.0",
    "redis": "^3.1.2",
    "request": "^2.88.2",
    "request-promise": "^4.2.6",
    "rimraf": "^3.0.2",
    "sequelize": "^6.21.2",
    "sequelize-cli": "^6.4.1",
    "spotify-web-api-node": "^5.0.2",
    "supertest": "^6.2.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.18.6",
    "@babel/core": "^7.18.6",
    "@babel/eslint-parser": "^7.18.2",
    "@babel/eslint-plugin": "^7.17.7",
    "@babel/preset-env": "^7.18.6",
    "@babel/register": "^7.18.6",
    "babel-preset-env": "^1.7.0",
    "babel-watch": "^7.7.0",
    "eslint-plugin-jsx-a11y": "^6.6.0",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "mocha": "^10.0.0",
    "start-server-and-test": "^1.14.0"
  }
}
